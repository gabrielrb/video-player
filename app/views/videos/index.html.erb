<div class="container">
  <div class="d-flex justify-content-center">
    <h2 class="mt-5">All videos</h2>
  </div>
  <div class="row">
  <% if @videos.present? %>
    <% @videos.each do |video| %>
          <div>
            <h2 class="border-bottom"><%= video.name %></h2>
            <p><strong>Uploader:</strong> <%= video.user.name %></p>
          </div>
      <% end %>
    <% end %>
  </div>
</div>
